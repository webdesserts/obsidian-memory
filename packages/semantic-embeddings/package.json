{
  "name": "@webdesserts/obsidian-memory-semantic-embeddings",
  "version": "0.1.0",
  "type": "module",
  "description": "Rust-based semantic embeddings for Obsidian notes",
  "main": "pkg/semantic_embeddings.js",
  "types": "pkg/semantic_embeddings.d.ts",
  "files": [
    "pkg",
    "scripts"
  ],
  "scripts": {
    "build": "wasm-pack build --target bundler --out-dir pkg",
    "build:debug": "wasm-pack build --target bundler --dev --out-dir pkg",
    "download-model": "node scripts/download-model.js",
    "prepare": "node scripts/download-model.js",
    "test": "wasm-pack test --node"
  },
  "keywords": [
    "embeddings",
    "semantic-search",
    "rust",
    "wasm",
    "obsidian"
  ],
  "author": "Michael Mullins",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
