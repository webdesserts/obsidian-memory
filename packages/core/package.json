{
  "name": "@webdesserts/obsidian-memory-core",
  "version": "0.1.0",
  "type": "module",
  "description": "Core Rust utilities for Obsidian Memory (wiki-links, file ops, embeddings)",
  "main": "pkg/obsidian_memory_core.js",
  "types": "pkg/obsidian_memory_core.d.ts",
  "files": [
    "pkg"
  ],
  "scripts": {
    "build": "wasm-pack build --target bundler --out-dir pkg crates/wasm",
